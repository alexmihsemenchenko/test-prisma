{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "generated/prisma/**/*.js",
      "generated/prisma/**/*.wasm",
      "generated/prisma/**/*.mjs",
      "generated/prisma/package.json"
    ],
    "watchAssets": true
  }
}
